<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ahmadlap.database" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewStudentList"
            android:label="@string/title_activity_view_student_list" >
        </activity>
        <activity
            android:name=".InsertStudentActivity"
            android:label="@string/title_activity_insert_student" >
        </activity>
        <activity
            android:name=".ShowStudentActivity"
            android:label="@string/title_activity_show_student" >
        </activity>
        <activity
            android:name=".SearchName"
            android:label="@string/title_activity_search_name" >
        </activity>
    </application>

</manifest>
